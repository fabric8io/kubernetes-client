{
  "apiVersion": "config.openshift.io/v1",
  "kind": "ImageDigestMirrorSet",
  "metadata": {
    "name": "ubi9repo"
  },
  "spec": {
    "imageDigestMirrors": [
      {
        "mirrors": [
          "example.com/example/ubi-minimal"
        ],
        "source": "registry.access.redhat.com/ubi9/ubi-minimal",
        "mirrorSourcePolicy": "AllowContactingSource"
      }
    ]
  }
}
