{
  "apiVersion": "gateway.networking.k8s.io/v1beta1",
  "kind": "HTTPRoute",
  "metadata": {
    "name": "example-route"
  },
  "spec": {
    "parentRefs": [
      {
        "name": "example-gateway"
      }
    ],
    "hostnames": [
      "example.com"
    ],
    "rules": [
      {
        "backendRefs": [
          {
            "name": "example-svc",
            "port": 80
          }
        ]
      }
    ]
  }
}
